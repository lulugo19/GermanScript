{
    "name": "germanskript-syntax-highlighting",
    "displayName": "GermanSkript Syntax Highlighting",
    "description": "Syntax-Highlighting f√ºr GermanSkript.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "germanskript",
